{
  "name": "acme/testing-demo",
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Acme\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration tests/phpunit.xml",
    "test:coverage": "XDEBUG_MODE=coverage phpunit --configuration tests/phpunit.xml --coverage-text --coverage-html build/coverage"
  }
}